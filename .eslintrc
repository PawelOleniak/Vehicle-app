{
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "babel-eslint",
      "extends": ["react-app",
        "react-app/jest",
        "plugin:prettier/recommended",
        "prettier"
      ],
      "plugins": ["prettier"],
      "rules": {
        "prettier/prettier": ["warn",{
          "semi": true,
          "singleQuote": true,
          "tabWidth": 2,
          "endOfLine":"auto",
          "useTabs": false}]
      }
    }